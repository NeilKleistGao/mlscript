:NewDefs


class Foo(x: int)
//│ class Foo(x: int)



class Bar(x: int, y: int) extends Foo(x + y)
//│ class Bar(x: int, y: int) extends Foo


mixin AA(a: int) {
}
//│ mixin AA(a: int)

mixin BB {}
//│ mixin BB()


class C(x: int) extends BB
//│ class C(x: int)

class D(x: int) extends AA(x)
//│ class D(x: int)

class E(x: int) extends BB, AA(x)
//│ class E(x: int)


