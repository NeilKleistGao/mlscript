:NewDefs


mixin Over {
  fun p = "hi"
}
//│ mixin Over() {
//│   fun p: "hi"
//│ }


class Base1(p: int) extends Over {
  fun test = [p, this.p]
  fun test2 = super.p
}
//│ class Base1(p: int) {
//│   fun p: "hi"
//│   fun test: (int, int,)
//│   fun test2: int
//│ }


Base1(123).test
//│ (int, int,)
//│ res
//│     = [ 123, 123 ]

Base1(123).test2
//│ int
//│ res
//│     = 'hi'


